{
	"name": "@clippo/fs",
	"version": "0.0.1",
	"keywords": [
		"fylesystem",
		"localstorage",
		"os"
	],
	"description": "File system functions for utilities. Created for CLIPPO, designed for cli and backend libraries",
	"homepage": "https://clippo.pigeonposse.com/",
	"repository": {
		"type": "git",
		"url": "https://github.com/pigeonposse/clippo",
		"directory": "packages/fs"
	},
	"license": "GPL-3.0",
	"type": "module",
	"exports": {
		"./types": "./dist/main.d.ts",
		".": "./dist/main.js"
	},
	"main": "dist/main.js",
	"module": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"bench": "node .dev/bench.js $@",
		"build": "vite build",
		"dev": "pnpm build && pnpm preview",
		"doc": "typedoc src/main.ts",
		"example": "node examples/main.js $@",
		"lint": "eslint . --ext .ts",
		"preview": "node dist/main.js $@",
		"test": "vitest run",
		"watch": "vitest watch"
	},
	"dependencies": {
		"conf": "13.0.1",
		"js-yaml": "4.1.0",
		"node-localstorage": "3.0.5",
		"open": "10.1.0",
		"toml": "3.0.0"
	},
	"devDependencies": {
		"@clippo/config": "workspace:*",
		"@types/conf": "^3.0.0",
		"@types/js-yaml": "4.0.9",
		"@types/node-localstorage": "1.3.3"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"extra": {
		"productName": "File System",
		"emoji": "ðŸ“‚"
	}
